# VARIABLE HELPERS

fn *clone x
	var (deref x)

# VARIABLE DICT HELEPRS

fn *dict (.. args)
	var
		apply dict args

fn *has (*d key)
	has (deref *d) key

fn *set (*d key value)
	swap *d set key value

fn *get (*d key)
	get (deref *d) key

fn *keys (*d)
	keys (deref *d)

# VARIABLE LIST HELPERS

fn *list (.. args)
	var
		apply list args

fn *append (*l value)
	swap *l append value

fn *count (*l)
	count (deref *l)

fn *map (*l func)
	map (deref *l) func